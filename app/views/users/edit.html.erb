<div class='userupdate'>
<div class="class">
	<br>
	<div class="row">
		<div class="col-md-4 col-md-offset-4">
			<div class=updateprofile>Complete Your Profile</div> <br>
			  <%= form_for @user, method: :patch do |f| %>
			   <h2><%= f.text_field :name, class:"form-control", :placeholder => "First name" %><br></h2>
			  <%= f.text_field :username, class:"form-control", :placeholder => "username" %><br>
			  <%= f.email_field :email, class:"form-control", :placeholder => "Email" %><br>

        
			   <%= f.text_field :profession, class:"form-control", :placeholder => "Profession" %><br>
			   <%= f.text_field :age, class:"form-control", :placeholder => "Age" %><br>
			   <%= f.text_field :gender, class:"form-control", :placeholder => "Gender" %><br>
			   <%= f.text_field :height, class:"form-control", :placeholder => "Height" %><br>
			   <%= f.text_field :education, class:"form-control", :placeholder => "Eduction" %><br>
			  <%= f.text_field :country, class:"form-control", :placeholder => "Country" %><br>
		
              <%= f.select :city_id, options_from_collection_for_select(City.all, "id", "name") %>
			  <%= f.text_field :number_of_children, class:"form-control", :placeholder => "Number of Children" %>
			   <%= f.text_field :bio, class:"form-control", :placeholder => "Bio" %>
			    <%= f.text_field :image_url, class:"form-control", :placeholder => "Profile Picture" %>
              <%= f.submit "Save Changes", class: "btn-submit" %>
			<% end %>
			</div>
		</div>
	</div>
</div>
